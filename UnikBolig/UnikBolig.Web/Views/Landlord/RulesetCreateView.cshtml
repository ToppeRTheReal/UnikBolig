@model UnikBolig.Models.EstateRulesetModel;


        <form asp-controller="Landlord" asp-action="CreateRuleset">


            <div class="row text-left">
                <div class="col-md-6">
                    <div class="card">
                        <div class="card-body">
                            <div class="">
                                <span> Navn på regelsættet <input class="form-control" asp-for="Name" /></span>
                            </div>

                            <div class="">
                                <input class="form-check-input" type="checkbox" asp-for="Dog">
                                <label class="form-check-label">
                                    Må Der være hund i husstanden?
                                </label>
                            </div>

                            <div class="">
                                <input class="form-check-input" type="checkbox" asp-for="Cat">
                                <label class="form-check-label">
                                    Må Der være kat i husstanden?
                                </label>
                            </div>

                            <div class="">
                                <input class="form-check-input" type="checkbox" asp-for="Creep">
                                <label class="form-check-label">
                                    Må Der være krybdyr i husstanden?
                                </label>
                            </div>

                            <div class="">
                                <input class="form-check-input" type="checkbox" asp-for="Fish">
                                <label class="form-check-label">
                                    Må Der være fisk i husstanden?
                                </label>
                            </div>

                            <div class="">
                                <button class="btn btn-primary btn-block"> Opret regelsæt </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </form>

